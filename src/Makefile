run:
	java -cp .:org.json.jar sim.Simulator --teams g5 random random random random random random random random random -r 30 -s 10 -l log.txt -e games.csv

gui:
	java -cp .:org.json.jar sim.Simulator --teams g5 random random random random random random random random random -r 10 -s 10 -f 15 --gui -c -l log.txt -e games.csv

compile:
	javac -cp .:org.json.jar */*.java

clean:
	rm */*.class
